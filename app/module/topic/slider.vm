#if($content&&$content.size()==1)
    #set($banners=$content.get(0))
    #if(${banners}&&${banners.size()}>0)
    <div class="slider cms-module-slider">
        <ul class="slider-img">
            #set($count = 0)
            #foreach($bannerEl in $banners)
                #set($count = $count + 1)
                <li class="#if($velocityCount==1) active #end"><a href="$!{bannerEl.get("link")}" target="_blank"
                                                                  style="background-image: url($!{bannerEl.get("imgUrl")});"
                                                                  alt="$!{bannerEl.get("name")}" title="$!{bannerEl.get("name")}"></a></li>
            #end
        </ul>
        <div class="pm">
            <a href="javascript:" class="btn btn-prev">向前</a>
            <a href="javascript:" class="btn btn-next">向后</a>
        </div>
        <ul class="slider-num">
            #set($_range = [1..$count])
            #foreach($_i in $_range)
                <li></li>
            #end
        </ul>
    </div>
    #end
#end