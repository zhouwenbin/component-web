<div class="page">
  <a href="#" class="pagination-prev">&lt;&lt;上一页</a>

  {{#each sarr}}
  <a href="#" data-target="{{.}}" class="{{equal current .}} pagination-set-page">{{.}}</a>
  {{/each}}

  {{#requireMore}}
  <span>...</span>
  {{/requireMore}}

  {{#each larr}}
  <a href="#" data-target="{{.}}" class="{{equal current .}} pagination-set-page">{{.}}</a>
  {{/each}}

  <a href="#" class="pagination-next">下一页&gt;&gt;</a>
</div>