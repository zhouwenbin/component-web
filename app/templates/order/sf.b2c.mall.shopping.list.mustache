<table cellpadding="0" cellspacing="0" class="gwc_tb1">
  <tbody>
    <tr>
      <td class="tb1_td1">
        <input type="checkbox" class="allselect order-select-all" can-value="selectAll">全选
      </td>
      <td class="tb1_td3" style="width:645px;">商品</td>
      <td class="tb1_td5">数量</td>
      <td class="tb1_td6">价格</td>
    </tr>
  </tbody>
</table>

{{#each products}}
<div style="padding:20px 0 20px 0;border:1px solid #EAEAEA; ">
  <table cellpadding="0" cellspacing="0" class="gwc_tb2">
    <tbody>
      <tr>
        <td class="tb2_td1">
        {{#limit}}
          <input type="checkbox" can-value="selected" class="order-select-one">
        {{/limit}}
        </td>
        <td class="tb2_td2">
          <div class="clearfix order-shop" style="position:relative;">
            <a target="_blank" href="/detail.html?sku={{skuId}}" class="fl order-shop-img">
              {{#sf.contain.img img}}<img src="{{sf.img img}}">{{/sf.contain.img}}
            </a>
            <div class="fl order-r">
              <span>
                <a target="_blank" href="/detail.html?sku={{skuId}}">{{name}}</a>
              </span> <font>{{#each attrs}}
                <span>{{specName}}: {{specValue}}</span>
                {{/each}}</font>
              <div class="clearfix">
                <span style="font-family:'宋体'; float:left; color:#B8B8B8;">
                  <img class="sf-country-icon-s" src="{{sf.img seller.nationLogoUrl}}">{{seller.nationName}}
                </span>
                <span class="fl s-souqing"> <em></em>
                  {{!status}}
                </span>
              </div>
            </div>
            {{#car-status limit}}
            <div class="t-Sold-out">售罄</div>
            {{/sf-status}}
          </div>
        </td>
        <td class="tb1_td5">
          <div class="clearfix num-bj">
            <input name="" type="button" class="num_oper fl order-min-btn" value="-" data-index="{{@index}}">
            <input name="" type="text" class="input_txt fl" can-value="num" data-index="{{@index}}">
            <input name="" type="button" class="num_oper fl order-add-btn" value="+" data-index="{{@index}}"></div>
          <div class="clearfix t-function">
            <a href="#" data-index="{{@index}}" class="order-delete-item-btn">删除</a>
            <!-- <a href="#">收藏</a> -->
          </div>
          {{#car-limit limit}}
          <div class="The-purchase">每人限购{{limit}}件</div>
          {{/car-limit}}
        </td>
        <td class="tb1_td6">
          <dl class="clearfix">
            <dt class="fl">商品单价：</dt>
            <dd>
              <label id="total1" class="tot" style="color:#959494;font-size:12px;">{{sf.price price}}</label>
              <!-- <s>￥{{sf.price oprice}}</s> -->
            </dd>
          </dl>
          <!--
          <dl class="clearfix">
            <dt class="fl">国际运费：</dt>
            <dd>￥{{!postage}}</dd>
          </dl>
          -->
          <dl class="clearfix">
            <dt class="fl">金额小计：</dt>
            <dd>
              <label id="total1-1" class="tot" style="color:#303030;font-size:14px; font-family:'微软雅黑';">{{&sf.price sum}}</label>
            </dd>
          </dl>
        </td>
      </tr>
    </tbody>
  </table>
</div>
{{/each}}
