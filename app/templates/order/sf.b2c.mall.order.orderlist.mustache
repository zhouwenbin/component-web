<div class="table table-1">
          <div class="table-h clearfix">
            <div class="table-c1 fl">
              订单信息
            </div>
            <div class="table-c2 fl">
              收货人
            </div>
            <div class="table-c3 fl">
              订单金额
            </div>
            <div class="table-c4 fl">
              订单状态
            </div>
            <div class="table-c5 fl">
              操作
            </div>
          </div>
          <ul class="table-b">
            {{#each orderlist}}
            <li class="clearfix">
              <div class="table-c1 fl">
                <div class="table-r1 clearfix">
                  <div class="table-img fl">
                    <a href="#"><img src="{{imageUrl}}" alt="" width="68" height="42" /></a><span></span>
                  </div>
                  <div class="table-text">
                    <h3><a href="#" title="{{goodsName}}">{{goodsName}}</a></h3>
                    <p>{{! <img src="img/icon-usa.png" alt="美国国旗" width="19" height="13" /> }}{{spec}}</p>
                  </div>
                </div>
                <div class="table-r2">订单号：{{orderId}}<span>下单日期：{{sf.time gmtPaid}}</span></div>
              </div>

              <div class="table-c2 fl">
                {{orderAddressItem.revice_name}}
              </div>
              <div class="table-c3 fl">
                ￥{{sf.price totalPrice - discount}}
              </div>
              <div class="table-c4 fl">
                {{orderStatus}}
                {{#if showRouter}}
                <div class="table-1-logistics" data-orderid="{{orderId}}">
                  <a href="javascript:return false;">查看物流</a>
                  <div class="tooltip" id="traceList">
                    努力加载中
                  </div>
                </div>
                {{/if}}
              </div>
              <div class="table-c5 fl" id="operationarea" data-orderid='{{orderId}}'>
                {{{optionHMTL}}}
              </div>
            </li>
            {{/each}}
          </ul>
        </div>