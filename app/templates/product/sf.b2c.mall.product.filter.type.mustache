<div class="hd fl clearfix">
  <ul>
    {{#each types}}
    <li class="{{#on}}on{{/on}}">
      <a href="#" {{data 'type'}} class="filter-type">{{name}}</a>
    </li>
    {{/each}}
  </ul>
</div>
<p class="ui-page-s fr">
  <b class="ui-page-s-len">{{page}}/{{allpage}}</b>
  <b class="ui-page-s-prev filter-type-prev" {{data 'filter'}} title="上一页">&lt;</b>
  <b class="ui-page-s-next filter-type-next" {{data 'filter'}} title="下一页">&gt;</b>
</p>
