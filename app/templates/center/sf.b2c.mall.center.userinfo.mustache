<h2>基本资料</h2>
{{^isModified}}
  <div class="order-b">
    <ul>
      <li class="active">
        <div class="order-r1 clearfix">
          <div class="order-r1c1 fl">
            <span>昵称：黄小牙</span>
            <span>性别：女</span>
          </div>
          <div class="order-r1c2 fl">
            <a class="order-edit" href="" id="user-info-modify-btn">修改</a>
          </div>
        </div>
      </li>
    </ul>
  </div>
{{/isModified}}
{{#isModified}}
  <div class="order-b center-info">
    <ul>
      <li class="active">
        <div class="order-r1 clearfix">
            <div class="order-r1c1 fl">
              昵称：<input type="text" class="input" can-value="input.nick" value="周文斌">
              性别：
              <div class="btn btn-select btn-select-1" role="btn">
                <span class="btn-select-num">请选择</span><span class="icon icon16-1"></span>
                <ul style="display: none;">
                  <li><label>男<input type="radio" can-value='input.gender' value='MALE' name="gender" class="visuallyhidden"></label></li>
                  <li><label>女<input type="radio" can-value='input.gender' value='FEMALE' name="gender" class="visuallyhidden"></label></li>
                </ul>
              </div>
            </div>
            <div class="order-r1c2 fr">
              <a href="#" class="btn btn-send" id="user-info-confirm-btn">保存</a>
            </div>
        </div>
      </li>
    </ul>
  </div>
{{/isModified}}