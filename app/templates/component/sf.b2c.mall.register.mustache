<!--注册-->
<div class="mask"></div>
<div class="register">
  <div class="register-h">
    <h2>抢先预约</h2>
    <a href="#" class="btn btn-close">关闭</a>
  </div>
  <!--手机注册-->
  <div class="register-b active">
    <ol>
      <li>
        <input id="input-mobile-num" placeholder="仅限中国大陆手机号" type="text" can-value="user.mobileNum" />{{#mobileNum}}<span id="mobileNumErrorTips" class="icon icon26">{{numError}}</span>{{/mobileNum}}
        <!--可发送时--<a href="#" class="btn btn-send active">发送验证码</a><!--可发送时-->
      </li>

      <li>
        <div class="clearfix">
            <div class="fl"><input id="input-mobile-code" placeholder="输入短信验证码" type="text" class="input1" can-value="user.mobileCode" /></div>
            <div class="fr"><button id="btn-send-mobilecode" state="false" class="btn btn-send disable fr" disabled="disabled">发送验证码</button></div>
        </div>
        {{#mobileCode}}<span id="mobileCodeErorTips" class="icon icon26">{{codeError}}</span>{{/mobileCode}}
        <!--可发送时--<a href="#" class="btn btn-send active">发送验证码</a><!--可发送时-->
      </li>
      <li>
        <input id="input-user-password" type="password" placeholder="设置6-18位字母、数字或标点符号的密码" can-value="user.password" />{{#password}}<span id="pwdErrorTips" class="icon icon26">{{pwdError}}</span>{{/password}}
      </li>
    </ol>
    <div class="register-br1">
      <button class="btn btn-send btn-register">同意协议并注册</button>
    </div>
    <div class="register-br2 clearfix">
      <div class="register-br2c1 fl">
        <label><input type="checkbox" state="false" id="ischecked" checked="checked" />我已阅读<a href="agreement.html" target="_blank">《顺丰海淘注册协议》</a></label>
      </div>
    </div>

  </div>
  <!--手机注册-->

</div>
<!--注册-->