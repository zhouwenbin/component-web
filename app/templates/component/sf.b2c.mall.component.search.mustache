{{#itemSearch.totalHits}}
<div class="pb">
  <div class="classify">
    <div class="pm">
      <h2><strong>“{{searchData.q}}”</strong>有<span class="text-important">{{itemSearch.totalHits}}</span>条相关产品</h2>
      <ul class="classify-list">
        <li class="classify-select" style="display: block;">
          <label>已选条件：</label>
          <ul id="classify-select">
              <li><a class="btn-search" href="javascript:">品牌：<strong>花王</strong><span class="btn-search-close"></span></a></li>
          </ul>
        </li>
        <li class="clearfix classify-logo">
        {{#each itemSearch.aggregations}}
          {{#sf-isByBrand name}}
          <div class="classify-c1 fl">
            <label>品牌：</label>
              <ul id="classify-logo">
                {{#each buckets}}
                  <li><a href="javascript:"><img src="{{pic}}" alt="{{key}}"></a></li>
                {{/each}}
              </ul>
            </div>
            {{#sf-isShowMoreBrand buckets}}
            <div class="classify-c2 fr">
              <a href="javascript:"><span id="classify-more" class="classify-more"></span><span class="icon icon105"></span></a>
            </div>
            {{/sf-isShowMoreBrand}}
            {{/sf-isByBrand}}
          {{/each}}
        </li>

        <li class="classify-text">
          <label>分类：</label>
          <ul>
            {{#each itemSearch.aggregations}}
              {{#sf-isByCategory name}}
                {{#each buckets}}
                  <li><a href="javascript:">{{key}}</a></li>
                {{/each}}
              {{/sf-isByCategory}}
            {{/each}}
          </ul>
        </li>
        <li class="classify-text">
          <label>货源地：</label>
          <ul>
            {{#each itemSearch.aggregations}}
              {{#sf-isByGoodsOrigin name}}
                {{#each buckets}}
                    <li><a href="javascript:">{{key}}</a></li>
                {{/each}}
              {{/sf-isByGoodsOrigin}}
            {{/each}}
          </ul>
        </li>
      </ul>
    </div>
  </div>
</div>

<div class="pb">
  <div class="pm">
    <div class="product">
      <div class="mb">
        <ul class="clearfix product-list">
          {{#each itemSearch.results}}
            <li>
              <div class="product-r1">
                  <a href=""><img src="../img/product.jpg" srcset="../img/product.jpg 1x,../img/product.webp 1x" alt="" ></a><span></span>
                  <div class="product-new">新品</div>
              </div>
              <h3><a href="">白色恋人 巧克力夹心饼干12枚白色恋人 巧克力夹心饼干12枚</a></h3>
              <p><img src="../img/icon-usa.png" alt="" width="22" height="14" />广州保税仓发货</p>
              <div class="product-r2 clearfix">
                  <div class="product-r2c1 fl">
                      <span>￥</span><strong>129</strong><del>￥189</del>5折

                  </div>
                  <div class="product-r2c2 fr"><a href="javascript:" class="icon icon90">购买</a></div>
              </div>
            </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>
{{/itemSearch.totalHits}}
