#define($_content)
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta name="viewport" content="width=device-width">
    <title>顺丰海淘</title>
    $!{_const_indexCss}
</head>
<body>
<div id="content">
    <div class="sf-b2c-mall-header"></div>
    <div class="pb">
        <div class="sf-b2c-mall-main-slider serverRendered">
            <div class="slider">
                <ul class="slider-img">
                    #foreach ($_el in $sliders)
                        <li  #if($velocityCount==1)class="active"#end>
                            <a href="$!{_el.link}" style="background-image: url($!{_el.imgUrl})"></a>
                        </li>
                    #end
                </ul>
                <ul class="slider-num">
                    #foreach ($_el in $sliders)
                        <li #if($velocityCount==1)class="active"#end>
                            <a href="#"></a>
                        </li>
                    #end
                </ul>
                <div class="pm">
                <a href="#" class="btn btn-prev" style="left: -100px; opacity: 0;">向前</a>
                <a href="#" class="btn btn-next" style="right: -100px; opacity: 0;">向后</a>
                </div>
            </div>
        </div>
        <div class="pm">
            <!--限时特卖-->
            <div class="sf-b2c-mall-limitedtimesale serverRendered">
                <div class="product">
                  <div id="LimitedTimeSale" class="mh clearfix">
                      <h2 class="fl">限时特卖</h2>
                      <ul class="fl product-tab">
                          <li id="current"><a href="#">当前</a></li>
                          <li id="future"><a href="#">即将开始</a></li>
                      </ul>
                  </div>
                  <div class="mb">
                      <ul class="clearfix product-list" data-itemIds=$!{timeLimitedSaleItemIds}>
                          #foreach ($_el in $limitedTimeSale)
                              <li>
                                  <div class="product-r1">
                                      <a href="$!{_el.link}" target="_blank">
                                          <img src="$!{_el.imgUrl}" alt="">
                                      </a>
                                      <span></span>
                                  </div>
                                  <h3>
                                      <a href="$_el.link" target="_blank">$!{_el.title}</a>
                                  </h3>
                                  <p>$!{_el.subTitle}</p>
                                  <div class="product-r2 clearfix" >
                                      <div id="timeAndNation4ProductClient" data-contentType="$!{_el.contentType}" #if($_el.homepageProductInfo) data-itemId="$!{_el.homepageProductInfo.itemId}" #elseif($_el.homepageTopicInfo) data-topicId="$!{_el.homepageTopicInfo.topicId}" data-displayEndTime="$!{_el.displayEndTime}" #end class="product-r2c1 fl"><span class="icon icon4"></span></div>
                                      #if ($_el.homepageProductInfo)
                                          <div class="product-r2c2 fr">
                                            <span class="icon">
                                              <img src="$!{_el.homepageProductInfo.showNationalUrl}" alt="">
                                            </span>
                                            <span class="product-r2-time"></span>
                                          </div>
                                      #end
                                  </div>
                                  #if ($_el.homepageProductInfo)
                                      <div id="price4ProductClient" class="product-r3 clearfix" data-contentType="$!{_el.contentType}" data-itemId="$!{_el.homepageProductInfo.itemId}"></div>
                                  #elseif ($_el.homepageTopicInfo)
                                      <div class="product-r3 clearfix" data-contentType="$!{_el.contentType}" data-topicId="$!{_el.homepageTopicInfo.topicId}">
                                          <div class="product-r3c1 fl">
                                              <strong>￥$!{_el.homepageTopicInfo.price}起</strong>
                                          </div>
                                          <div class="product-r3c2 fr">
                                              <strong>$!{_el.homepageTopicInfo.discount}</strong>折
                                          </div>
                                      </div>
                                  #end
                              </li>
                          #end
                      </ul>
                  </div>
                </div>
            </div>
            <!--限时特卖-->

            <!--极速海淘-->
            <div class="sf-b2c-mall-rapidseabuy serverRendered">
                <div class="product">
                    <div id="RapidSeaBuy" class="mh">
                        <h2>极速海淘</h2>
                    </div>
                    <div class="mb">
                        <ul class="clearfix product-list" data-itemIds=${fastSaleInfoItemIds}>
                            #foreach ($_el in $fastSaleInfoList.fastSaleInfos )
                                <li>
                                    <div class="product-r1">
                                        <a href="$!{_el.link}" target="_blank"><img src="$!{_el.imgUrl}" alt=""></a>
                                        <span></span>
                                    </div>
                                    <h3>
                                        <a href="$!{_el.link}" target="_blank" title="$!{_el.title}">$!{_el.title}</a>
                                    </h3>

                                    <p>$!{_el.subTitle}</p>

                                    <div class="product-r2 clearfix">
                                        <div class="product-r2c1 fl">
                                            <span class="icon icon6">
                                              <b>$!{_el.limitedTime}</b>天到
                                              <i></i>
                                            </span>
                                        </div>
                                        #if ($_el.homepageProductInfo)
                                            <div class="product-r2c2 fr">
                                              <span class="icon icon5">
                                                <img src="$!{_el.homepageProductInfo.showNationalUrl}" alt="">
                                              </span>
                                            </div>
                                        #end
                                    </div>
                                    #if ($_el.homepageProductInfo)
                                        <div id="price4ProductClient" class="product-r3 clearfix" data-contentType="$!{_el.fastSaleContentType}" data-itemId="$!{_el.homepageProductInfo.itemId}"></div>
                                    #elseif ($_el.homepageTopicInfo)
                                        <div class="product-r3 clearfix" data-contentType="$!{_el.fastSaleContentType}" data-topicId="$!{_el.homepageTopicInfo.topicId}">
                                            <div class="product-r3c1 fl">
                                                <strong>￥$!{_el.homepageTopicInfo.price}起</strong>
                                            </div>
                                            <div class="product-r3c2 fr">
                                                <strong>$!{_el.homepageTopicInfo.discount}</strong>折
                                            </div>
                                        </div>
                                    #end
                                </li>
                            #end
                        </ul>
                    </div>
                    <div class="mf">
                      #if( $fastSaleInfoList.pageInfo.hasMore )
                      <a href="#" class="btn btn-more">查看更多</a>
                      #end
                    </div>
                </div>
            </div>
            <!--极速海淘-->
        </div>
    </div>
    <!--尾部-->
    <div class="sf-b2c-mall-footer"></div>

    <!--尾部-->
</div>
    $!{_const_indexScript}
</body>
#end
