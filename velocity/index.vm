<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta name="viewport" content="width=device-width">
<title>顺丰海淘</title>
<link href="css/reset.css" rel="stylesheet" type="text/css" />
<link href="css/style.css" rel="stylesheet" type="text/css" />
</head>
<body>
  <div id="content">
    <div class="sf-b2c-mall-header"></div>

    <div class="pb">
      <div class="sf-b2c-mall-main-slider">
        <div class="slider">
          <ul class="slider-img">
            #foreach ($el in $sliders)
            <li>
              <a href="$el.link" style="background-image: url($el.imgUrl)"></a>
            </li>
            #end
          </ul>
          <ul class="slider-num">
            #foreach ($el in $sliders)
              <li>
                <a href="###"></a>
              </li>
            #end
          </ul>
          <div class="pm">
            <a href="###" class="btn btn-prev" style="left: -100px; opacity: 0;">向前</a>
            <a href="###" class="btn btn-next" style="right: -100px; opacity: 0;">向后</a>
          </div>
        </div>
      </div>
      <div class="pm">
        <!--限时特卖-->
        <div class="sf-b2c-mall-limitedtimesale">
          <div class="product">
            <div id="LimitedTimeSale" class="mh clearfix">
              <h2 class="fl">限时特卖</h2>
              <ul class="fl product-tab">
                <li id="current"><a href="#">当前</a></li>
                <li id="future"><a href="#">即将开始</a></li>
              </ul>
            </div>
          </div>
          <div class="mb">
            <ul class="clearfix product-list">
            #foreach ($el in $limitedTimeSale)
              <li>
                <div class="product-r1">
                  <a href="$el.link" target="_blank">
                    <img src="$el.imgUrl" alt="">
                  </a>
                  <span></span>
                </div>
                <h3>
                  <a href="$el.link" target="_blank">$el.title</a>
                </h3>
                <p>$el.subTitle</p>
                <div class="product-r2 clearfix">
                  <div class="product-r2c1 fl"><span class="icon icon4"></span>$el.displayEndTime</div>
                  <div class="product-r2c2 fr"><span class="icon icon5"></span></div>
                </div>
                ＃if($el.homePageProductInfo)
                <div class="product-r3 clearfix" data-skuId="$el.homePageProductInfo.skuId"></div>
                #else if($el.homepageTopicInfo)
                <div class="product-r3 clearfix" data-topicId="$el.homepageTopicInfo.topicId">
                  <div class="product-r3c1 fl">
                    <strong>￥$el.homepageTopicInfo.price</strong>
                    <del>￥12</del>
                  </div>
                  <div class="product-r3c2 fr">
                    <strong>$el.homepageTopicInfo.discount</strong>折
                  </div>
                </div>
                #end
              </li>
            #end
            </ul>
          </div>
        </div>
        <!--限时特卖-->

        <!--极速海淘-->
        <div class="sf-b2c-mall-rapidseabuy">
          <div class="product">
            <div id="RapidSeaBuy" class="mh">
              <h2>极速海淘</h2>
            </div>
            <div class="mb">
              <ul class="clearfix product-list">
                #foreach ($el in $fastSaleInfoList.fastSaleInfos )
                <li>
                  <div class="product-r1">
                    <a href="$el.link" target="_blank"><img src="$el.imgUrl" alt=""></a>
                    <span></span>
                  </div>
                  <h3>
                    <a href="$el.link" target="_blank" title="$el.title">$el.title</a>
                  </h3>
                  <p>白色 上等牛皮 有质感 光泽度好 手感极佳</p>
                  <div class="product-r2 clearfix">
                    <div class="product-r2c1 fl">
                      <span class="icon icon6">
                        <b>22</b>天到
                        <i></i>
                      </span>
                    </div>
                    <div class="product-r2c2 fr">
                      <span class="icon icon5">
                        <img src="img/icon-usa.png" alt="">
                      </span>
                    </div>
                  </div>
                  ＃if($el.homePageProductInfo)
                  <div class="product-r3 clearfix" data-skuId="$el.homePageProductInfo.skuId"></div>
                  #else if($el.homepageTopicInfo)
                  <div class="product-r3 clearfix" data-topicId="$el.homepageTopicInfo.topicId">
                    <div class="product-r3c1 fl">
                      <strong>￥$el.homepageTopicInfo.price</strong>
                      <del>￥12</del>
                    </div>
                    <div class="product-r3c2 fr">
                      <strong>$el.homepageTopicInfo.discount</strong>折
                    </div>
                  </div>
                  #end
                </li>
                #end
              </ul>
            </div>
          </div>
        </div>
        <!--极速海淘-->
      </div>
    </div>
    <!--尾部-->
    <div class="sf-b2c-mall-footer"></div>

  <!--尾部-->
  </div>

  <script src="bower_components/requirejs/require.js"></script>
  <script src="scripts/base/sf.web.ver1.0.build1418617459405.js"></script>
  <script src="scripts/sf.b2c.mall.require.config.js"></script>
  <script src="scripts/page/sf.b2c.mall.page.main.js"></script>

  <script type="text/javascript">var cnzz_protocol = (("https:" == document.location.protocol) ? " https://" : " http://");document.write(unescape("%3Cscript src='" + cnzz_protocol + "v1.cnzz.com/z_stat.php%3Fid%3D1253511102%26show%3Dpic1' type='text/javascript'%3E%3C/script%3E"));</script>
</body>
</html>